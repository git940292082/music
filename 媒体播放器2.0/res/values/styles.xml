<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="ButtonMenu">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>  
        <item name="android:button">@null</item>
       	<item name="android:textSize">12sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:textColor">@drawable/select_color</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:background">#fff</item>
    </style>
<style name="MusicBt">
    <item name="android:textSize">18dp</item>
    <item name="android:textColor">@color/main_color</item>
    <item name="android:padding">10dp</item>
    <item name="android:background">@drawable/bt_music_select</item>

</style>
<style name="MusicLineBt">
    <item name="android:textSize">14dp</item>
    <item name="android:textColor">#666</item>
    <item name="android:padding">10dp</item>
    <item name="android:background">@drawable/bt_music_select2</item>

</style>
<style name="regist_tv">
    <item name="android:textSize">16sp</item>
    <item name="android:paddingTop">18dp</item>
    <item name="android:paddingRight">8dp</item>
    <item name="android:paddingBottom">8dp</item>
    <item name="android:background">#eee</item>
</style>

</resources>
